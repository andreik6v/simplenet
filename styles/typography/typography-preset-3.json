{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Space Grotesk",
	"slug": "typography-preset-3",
	"settings": {
		"typography": {
			"fontFamilies": [
			{
          "fontFace": [
            {
              "fontFamily": "Space Grotesk",
              "fontStyle": "normal",
              "fontWeight": "300 700",
              "src": [
                "file:./assets/fonts/space-grotesk/SpaceGrotesk[wght].woff2"
              ]
            }
          ],
          "fontFamily": "\"Space Grotesk\", sans-serif",
          "name": "Space Grotesk",
          "slug": "space-grotesk"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Space Mono",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/space-mono/SpaceMono-Regular.ttf"
              ]
            },
            {
              "fontFamily": "Space Mono",
              "fontStyle": "italic",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/space-mono/SpaceMono-Italic.ttf"
              ]
            },
            {
              "fontFamily": "Space Mono",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/space-mono/SpaceMono-Bold.ttf"
              ]
            },
            {
              "fontFamily": "Space Mono",
              "fontStyle": "italic",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/space-mono/SpaceMono-BoldItalic.ttf"
              ]
            }
          ],
          "fontFamily": "\"Space Mono\", monospace",
          "name": "Space Mono",
          "slug": "monospace"
        }
			],
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": false,
          "name": "Medium",
          "size": "1.05rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.165rem",
            "max": "1.365rem"
          },
          "name": "Large",
          "size": "1.365rem",
          "slug": "large"
        },
        {
		"fluid": {
			"min": "1.5rem",
			"max": "1.75rem"
		},
		"name": "Extra Large",
		"size": "1.75rem",
		"slug": "x-large"
	},
	{
		"fluid": {
			"min": "1.75rem",
			"max": "2.375rem"
		},
		"name": "Extra Extra Large",
		"size": "2.375rem",
		"slug": "xx-large"
	}
]
		}
	},
"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|monospace"
		},
		"elements": {
  "heading": {
        "typography": {
          "fontWeight": "500",
          "fontFamily": "var:preset|font-family|space-grotesk"
        }
      },
				"h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large",
          "lineHeight": "1.25"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large",
           "lineHeight": "1.25"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase",
          "letterSpacing": "1px"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase",
          "letterSpacing": "1px"
        }
      }
		}
}
}
