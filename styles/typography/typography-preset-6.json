{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Haskoy",
	"slug": "typography-preset-6",
	"settings": {
		"typography": {
			"fontFamilies": [
			{
          "fontFace": [
            {
              "fontFamily": "Haskoy",
              "fontStyle": "normal",
              "fontWeight": "100 800",
              "src": [
                "file:./assets/fonts/haskoy/Haskoy-variable.woff2"
              ]
            }
          ],
          "fontFamily": "\"Haskoy\", sans-serif",
          "name": "Haskoy",
          "slug": "haskoy"
        },
        {
          "fontFamily": "Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
			],
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": false,
          "name": "Medium",
          "size": "1.05rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.165rem",
            "max": "1.365rem"
          },
          "name": "Large",
          "size": "1.365rem",
          "slug": "large"
        },
        {
		"fluid": {
			"min": "1.5rem",
			"max": "1.75rem"
		},
		"name": "Extra Large",
		"size": "1.75rem",
		"slug": "x-large"
	},
	{
		"fluid": {
			"min": "1.75rem",
			"max": "2.375rem"
		},
		"name": "Extra Extra Large",
		"size": "2.375rem",
		"slug": "xx-large"
	}
]
		}
	},
"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|haskoy"
		},
		"elements": {
			"heading": {
					"typography": {
						"fontFamily": "var:preset|font-family|haskoy",
						"fontWeight": "600"
					}
				},
				"h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large",
          "lineHeight": "1.25",
           "letterSpacing": "-0.02em"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large",
           "lineHeight": "1.25"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase",
          "letterSpacing": "1px"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textTransform": "uppercase",
          "letterSpacing": "1px"
        }
      }
		}
}
}
