					"name": "InterVariable",
					"slug": "default"
        },
        {
          "fontFamily": "monospace",
          "name": "Monospace",
          "slug": "monospace"
        }
      ],
      "fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.05rem",
					"slug": "medium"
