{
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
  "version": 2,
  "customTemplates": [
    {
      "name": "page-no-title",
      "postTypes": ["page"],
      "title": "Page No Title"
    }
  ],
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultPalette": true,
      "link": true,
      "palette": [
        {
            "color": "oklch(51.06% 0.2301 276.97)",
            "name": "Accent 1",
            "slug": "accent-1"

          },
          {
            "color": "hsl(0 0% 100%)",
            "name": "Theme 1",
            "slug": "theme-1"
          },
          {
            "color": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 2%)",
            "name": "Theme 2",
            "slug": "theme-2"
          },
          {
            "color": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 10%)",
            "name": "Theme 3",
            "slug": "theme-3"
          },
          {
            "color": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 20%)",
            "name": "Theme 4",
            "slug": "theme-4"
          },
          {
            "color": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 80%)",
            "name": "Theme 5",
            "slug": "theme-5"
          },
          {
            "color": "hsl(0 0% 4%)",
            "name": "Theme 6",
            "slug": "theme-6"
          }
      ]
    },
    "custom": {
      "color": {
        "theme-1-dark": "hsl(0 0% 10%)",
        "theme-2-dark": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 96%)",
        "theme-3-dark": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 88%)",
        "theme-4-dark": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 75%)",
        "theme-5-dark": "color-mix(in srgb, var(--wp--preset--color--theme-1), var(--wp--preset--color--theme-6) 50%)",
        "theme-6-dark": "hsl(0 0% 100% / 85%)",
        "accent-1-dark": "color-mix(in srgb, var(--wp--preset--color--accent-1), var(--wp--preset--color--theme-1) 50%)"
      },
      "transition": {
        "duration": "200ms"
      }
    },
    "layout": {
      "contentSize": "640px",
      "wideSize": "1200px"
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "name": "1",
          "size": "1rem",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "1.5rem",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "min(2.5rem, 3vw)",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "min(4rem, 5vw)",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "min(6.5rem, 8vw)",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "min(10.5rem, 13vw)",
          "slug": "60"
        }
