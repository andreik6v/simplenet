					"size": "1.365rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.375rem"
					},
					"name": "Extra Extra Large",
					"size": "2.375rem",
					"slug": "xx-large"
				}
			],
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true
